@prefix acl: <http://www.w3.org/ns/auth/acl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

<#public>
    a acl:Authorization;
    acl:agentClass foaf:Agent;
    acl:default <../foo/>;
    acl:mode acl:Read.

<#owner>
    a acl:Authorization;
    acl:agent <https://michielbdejong.com/profile/card#me>;
    acl:default <../foo/>;
    acl:mode acl:Control.